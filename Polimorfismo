class Ave:
    def volar(self):
        return "Algunas aves pueden volar."

class Aguila(Ave):
    def volar(self):
        return "El águila vuela a grandes alturas."

class Pingüino(Ave):
    def volar(self):
        return "El pingüino no puede volar, pero nada muy bien."

# Uso de polimorfismo
aves = [Aguila(), Pingüino()]
for ave in aves:
    print(ave.volar())
